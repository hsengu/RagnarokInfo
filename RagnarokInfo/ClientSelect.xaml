<Window x:Name="ROinfo_Client_Select" x:Class="RagnarokInfo.ClientSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RagnarokInfo"
        mc:Ignorable="d"
        Title="ROinfo Client Select" Height="200" Width="200"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="label" Content="Client Selection" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20" Margin="10,10,0,0" Width="174" Foreground="Black"/>
        <Button x:Name="Renewal" Content="Renewal" HorizontalAlignment="Left" VerticalAlignment="Top" Width="52" Margin="70,50,0,0" Click="Renewal_Click" Background="White"/>
        <Button x:Name="Classic" Content="Classic" HorizontalAlignment="Left" VerticalAlignment="Top" Width="52" Margin="70,90,0,0" Click="Classic_Click" Background="White"/>
        <Button x:Name="Sakray" Content="Sakray" HorizontalAlignment="Left" VerticalAlignment="Top" Width="52" Margin="70,130,0,0" Click="Sakray_Click" Background="White"/>
    </Grid>
</Window>
